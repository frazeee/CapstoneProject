{
    "version": 2,
    "builds": [
      {
        "src": "server/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**/*.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }